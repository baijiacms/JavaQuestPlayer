    <script>


function isNeedRefresh()
{
try{
   $.getJSON("/engine/isNeedRefresh",function(data,status){
    var isFresh=false;
    if(data.varsdescchanged)
    {
   // isFresh=true;
    }
    if(data.maindescchanged)
